<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>plex</Name>
  <Description>Put a container Desctiption here.</Description>
  <Registry>https://registry.hub.docker.com/u/lonix/plex/</Registry>
  <Repository>lonix/plex:latest</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
    <Variable>
      <Name>PLEXPASS</Name>
      <Value>1</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>host</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/plex/</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/series-archive/</HostDir>
      <ContainerDir>/media/series-archive</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/series-childern/</HostDir>
      <ContainerDir>/media/series-childern</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/movies/</HostDir>
      <ContainerDir>/media/movies</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/movies-childern/</HostDir>
      <ContainerDir>/media/movies-childern</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/music/</HostDir>
      <ContainerDir>/media/music</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/network/tvshows</HostDir>
      <ContainerDir>/media/series</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/network/bilder</HostDir>
      <ContainerDir>/media/pictures</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>cf9cabf6</Version>
  <WebUI>http://[IP]:32400/web</WebUI>
  <Banner>https://raw.githubusercontent.com/binhex/docker-templates/master/binhex/images/plex-banner.png</Banner>
  <Icon>https://raw.githubusercontent.com/binhex/docker-templates/master/binhex/images/plex-icon.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
